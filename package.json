{
  "name": "password-manager",
  "author": "pandurx",
  "description": "password manager",
  "version": "0.1.0",
  "private": true,
  "main": "public/main.js",
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "bootstrap": "^5.2.0",
    "electron-updater": "^5.2.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start-electron": "electron .",
    "start": "react-scripts start",
    "build-react": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "electron-builder install-app-deps",
    "build": "electron-builder --windows=portable",
    "release": "electron-builder --windows --linux --publish always",
    "dist": "electron-builder --win=portable"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "extends": null,
    "appId": "org.pandurx.password-manager",
    "productName": "password-manager",
    "win": {
      "target": ["portable"]
    },
    "files": [
      "src/*",
      "public/main.js",
      "build/**/*",
      "node_modules/**/*",
      "dist/win-unpacked/resources/**",
      "package.json"
    ],
    "portable": {
      "artifactName": "portable.exe"
    }
  },
  "devDependencies": {
    "electron": "^20.1.3",
    "electron-builder": "^23.3.3"
  }
}
